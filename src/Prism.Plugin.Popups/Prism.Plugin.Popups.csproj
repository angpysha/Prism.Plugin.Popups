<Project Sdk="MSBuild.Sdk.Extras/2.0.54">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;Xamarin.iOS10;MonoAndroid90;MonoAndroid10.0</TargetFrameworks>
    <Description>Prism Forms Plugin to provide Popup Navigation working with the Rg.Plugins.Popup library</Description>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DebugType>pdbonly</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Prism.Forms" Version="8.0.0.1850-pre" />
    <PackageReference Include="Rg.Plugins.Popup.Angpysha" Version="2.0.0.7-angpysha" />
  </ItemGroup>

  <PropertyGroup>
    <AssemblyName>Prism.Plugins.Popup</AssemblyName>
    <RootNamespace>Prism.Plugins.Popup</RootNamespace>
    <PackageId>Prism.Plugins.Popup.Angpysha</PackageId>
    <Description>Plugin for Xamarin forms. Allows you to open any page as a popup.</Description>
    <Version>7.2.1047-angpysha</Version>
  </PropertyGroup>
</Project>
